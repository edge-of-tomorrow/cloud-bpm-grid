{
  "realm" : "${auth.realm}",
  "resource" : "process-server",
  "realm-public-key" : "${auth.public.key}",
  "auth-server-url" : "http://${auth.host}:${auth.port}/auth",
  "ssl-required" : "external",
  "enable-basic-auth" : true,
  "principal-attribute" : "preferred_username",
  "credentials" : {
    "secret" : "${auth.process.server.secret}"
  }
}
